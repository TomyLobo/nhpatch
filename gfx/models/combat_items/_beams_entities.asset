
# MUZZLE FLASHES beam_blue_01
entity = {
        name = "beam_blue_01_muzzle_entity"
        pdxmesh = "locator_mesh"
        
        default_state = "idle"
        state = { name = "idle" state_time = 3 looping = no
                event = { time = 0 particle = "beam_blue_01_muzzle_particle" keep_particle = yes sound = { soundeffect = beam_blue_01 } }
                event = { trigger_once = yes sound = { soundeffect = "distance_beam_blue_01" } } 
        }
}

# HIT ENTITIES beam_blue_01
